{
  "name": "tthub",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "set NODE_OPTIONS='--import ./instrument.server.mjs' && vite dev --port 3000",
    "build": "vite build && cp instrument.server.mjs .output/server",
    "serve": "vite preview",
    "test": "vitest run",
    "test:e2e": "npm run test:e2e:setup && playwright test",
    "test:e2e:setup": "npm run test:e2e:db:reset && npm run test:e2e:db:seed",
    "test:e2e:db:reset": "set DATABASE_URL=file:./prisma/test.db && npx prisma migrate deploy",
    "test:e2e:db:seed": "set DATABASE_URL=file:./prisma/test.db && npx tsx prisma/seed.ts",
    "test:e2e:server": "set DATABASE_URL=file:./prisma/test.db && npm run dev",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:migrate:reset": "dotenv -e .env.local -- prisma migrate reset",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:seed": "dotenv -e .env.local -- prisma db seed",
    "db:format": "dotenv -e .env.local -- prisma format",
    "generateVapidKeys": "node --import ./generateVapidKeys.js",
    "start": "node --import ./.output/server/instrument.server.mjs .output/server/index.mjs"
  },
  "dependencies": {
    "@fullcalendar/core": "6.1.20",
    "@fullcalendar/daygrid": "6.1.20",
    "@fullcalendar/react": "6.1.20",
    "@prisma/adapter-better-sqlite3": "7.2.0",
    "@prisma/client": "7.2.0",
    "@sentry/tanstackstart-react": "10.34.0",
    "@tailwindcss/vite": "4.1.18",
    "@tanstack/match-sorter-utils": "8.19.4",
    "@tanstack/nitro-v2-vite-plugin": "1.141.0",
    "@tanstack/react-devtools": "0.9.2",
    "@tanstack/react-form": "1.27.7",
    "@tanstack/react-query": "5.90.18",
    "@tanstack/react-router": "1.139.16",
    "@tanstack/react-router-devtools": "1.139.15",
    "@tanstack/react-router-ssr-query": "1.139.14",
    "@tanstack/react-start": "1.139.14",
    "@tanstack/react-table": "8.21.3",
    "@tanstack/router-plugin": "1.139.14",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "daisyui": "5.5.14",
    "dotenv": "17.2.3",
    "lucide-react": "0.562.0",
    "mustache": "4.2.0",
    "node-device-detector": "2.2.5",
    "open-holiday-js": "1.0.3",
    "prisma": "7.2.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sonner": "2.0.7",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "1.4.0",
    "vite-tsconfig-paths": "6.0.4",
    "web-push": "3.6.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@catppuccin/daisyui": "2.1.1",
